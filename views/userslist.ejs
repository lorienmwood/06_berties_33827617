<!DOCTYPE html>
<html>

<head>
    <title>
        <%=shopData.shopName%>: Registered Users
    </title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
</head>

<body>
    <h1>
        <%=shopData.shopName%>: Registered Users
    </h1>

    <table>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Username</th>
            <th>Email</th>
        </tr>

        <% users.forEach(function(user) { %>
            <tr>
                <td>
                    <%= user.firstName %>
                </td>
                <td>
                    <%= user.lastName %>
                </td>
                <td>
                    <%= user.username %>
                </td>
                <td>
                    <%= user.email %>
                </td>
            </tr>
            <% }) %>
    </table>
</body>

</html>